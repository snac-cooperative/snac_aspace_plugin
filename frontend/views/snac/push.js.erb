<% if res[:error] != '' %>
  AS.openQuickModal(AS.renderTemplate("template_snac_job_failure_title"), '<%= res[:error] %>');
<% else %>
  AS.openQuickModal(AS.renderTemplate("template_snac_job_success_title"), AS.renderTemplate("template_snac_job_success_message"));
  setTimeout(function() {
    window.location = '<%= res[:job_uri] %>';
  }, 2000);
<% end %>
